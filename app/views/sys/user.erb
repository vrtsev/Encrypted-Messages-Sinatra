<h1>Hello, <%= @logged_in_as.first_name %>!</h1>
<hr>

<div class="bs-example" data-example-id="table-within-panel">
  <div class="panel panel-default">
    <div class="panel-heading"><%= @logged_in_as.first_name %></div>
      <div class="panel-body">
        <p>Here's the information we have for you:</p>
      </div>

      <table class="table">
        <thead>
          <tr>
            <th>#</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email Address</th>
            <th>Registered At</th>
            <th>Updated At</th>
          </tr>
        </thead>
      <tbody>
        <tr>
          <th scope="row"><%= @logged_in_as.id %></th>
          <td><%= @logged_in_as.first_name %></td>
          <td><%= @logged_in_as.last_name %></td>
          <td><%= @logged_in_as.email %></td>
          <td><%= @logged_in_as.created_at %></td>
          <td><%= @logged_in_as.updated_at %></td>
        </tr>
      </tbody>
    </table>
  </div>
</div>